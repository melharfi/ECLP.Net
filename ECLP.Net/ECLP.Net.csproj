<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Tool to read and parse command line or parse a file with many command lines.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/melharfi/ECLP.Net</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/melharfi/ECLP.Net</RepositoryUrl>
    <PackageTags>Easy Command Line Parser,ECLP,CLP,Command Line Parser</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\ECLP\resources\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
